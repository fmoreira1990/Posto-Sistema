  ClientClassesUnit1 in '..\Client\ClientClassesUnit1.pas',
  ClientModuleUnit2 in '..\Client\ClientModuleUnit2.pas' {ClientModule2: TDataModule};

{$R *.res}

begin
  if WebRequestHandler <> nil then
    WebRequestHandler.WebModuleClass := WebModuleClass;
  Application.Initialize;
  Application.CreateForm(TPostoServerMain, PostoServerMain);
  Application.Run;
end.
