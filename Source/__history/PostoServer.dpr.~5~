program PostoServer;
{$APPTYPE GUI}

{$R *.dres}

uses
  Vcl.Forms,
  Web.WebReq,
  IdHTTPWebBrokerBridge,
  uPostoServerMain in 'uPostoServerMain.pas' {PostoServerMain},
  uDsmBase in 'uDsmBase.pas' {DsmBase: TDSServerModule},
  uPostoServerModule in 'uPostoServerModule.pas' {PostoServerModule: TDataModule},
  uServerModule in 'uServerModule.pas' {WebModule1: TWebModule},
  uDsmProduto in 'uDsmProduto.pas' {DsmProduto: TDSServerModule};

{$R *.res}

begin
  if WebRequestHandler <> nil then
    WebRequestHandler.WebModuleClass := WebModuleClass;
  Application.Initialize;
  Application.CreateForm(TPostoServerMain, PostoServerMain);
  Application.Run;
end.
