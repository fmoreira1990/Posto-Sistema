program PostoServer;
{$APPTYPE GUI}

{$R *.dres}

uses
  Vcl.Forms,
  Web.WebReq,
  IdHTTPWebBrokerBridge,
  uPostoServerMain in 'uPostoServerMain.pas' {Form1},
  ServerMethodsUnit1 in 'ServerMethodsUnit1.pas' {DsmBase: TDSServerModule},
  uPostoServerMetods in 'uPostoServerMetods.pas' {ServerContainer1: TDataModule},
  uPostoServerDAO in 'uPostoServerDAO.pas' {WebModule1: TWebModule},
  uDsmProduto in 'uDsmProduto.pas' {DsmProduto: TDSServerModule};

{$R *.res}

begin
  if WebRequestHandler <> nil then
    WebRequestHandler.WebModuleClass := WebModuleClass;
  Application.Initialize;
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
